# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME=CamRove
NEXT_PUBLIC_DEFAULT_CITY=Kolkata

# Payment Configuration
NEXT_PUBLIC_PAYMENT_MODE=deposit
NEXT_PUBLIC_BOOKING_DEPOSIT=999

# Razorpay (for production - add your keys later)
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_your_key_here
RAZORPAY_KEY_SECRET=your_razorpay_secret_here

# WhatsApp API (for production)
WHATSAPP_API_URL=https://graph.facebook.com/v17.0
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number

# Google Business API (for reviews)
GOOGLE_BUSINESS_API_KEY=your_google_business_api_key

# Optional: Cloudinary for media storage
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Development flags
NEXT_PUBLIC_USE_DUMMY_DATA=true
NEXT_PUBLIC_ENABLE_DEBUG_LOGS=false